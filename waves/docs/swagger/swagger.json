    "basePath": "/",
    "paths": {
        "/waves/balance/{address}": {
            "get": {
                "description": "return balance of account in Waves for specific node",
                "produces": [
                    "application/json"
                ],
                "summary": "Waves balance of account",
                "parameters": [
                    {
                        "type": "string",
                        "description": "address",
                        "name": "address",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/responses.BalanceResponse"
                            }
                        }
                    }
                }
            }
        },
        "/waves/balances": {
            "post": {
                "description": "return balances of accounts in Waves",
                "produces": [
                    "application/json"
                ],
                "summary": "Waves balance of accounts by list",
                "parameters": [
                    {
                        "description": "addressesArray",
                        "name": "addressesArray",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "object"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/responses.BalancesResponse"
                            }
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "responses.BalanceResponse": {
            "type": "object",
            "properties": {
                "balance": {
                    "type": "string"
                }
            }
        },
        "responses.BalancesResponse": {
            "type": "object",
            "properties": {
                "balances": {
                    "type": "object"
                }
            }
        }
    }
}